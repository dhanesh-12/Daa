S = list(map(int, input("Enter the elements of set S separated by space: ").split()))
d = int(input("Enter the target sum d: "))

def sum_of_subset(S, d):
    result = []
    
    def backtrack(start, subset, total):
        if total == d:
            result.append(subset[:])
            return
        if total > d:
            return
        for i in range(start, len(S)):
            subset.append(S[i])
            backtrack(i + 1, subset, total + S[i])
            subset.pop()
    
    backtrack(0, [], 0)
    return result

# Find and display subsets
subsets = sum_of_subset(S, d)
if subsets:
    print(f"Subsets whose sum is {d}:")
    for subset in subsets:
        print(subset)
else:
    print(f"No subset found with sum: {d}")
